import java.util.Scanner;

public class exerc_02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		int resultado = sc.nextInt();
		
		resultado = x + y;
		
		System.out.println("A soma é: " + resultado);
		
		
		sc.close();
		

	}

}
